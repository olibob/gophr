{
    "family": "gophrFrontApp",
    "containerDefinitions": [{
        "name": "gophr",
        "image": "%REPOSITORY_URI%:%BUILD_NUMBER%",
        "cpu": 20,
        "memory": 256,
        "portMappings": [{
            "containerPort": 3000,
            "hostPort": 3000,
            "protocol": "tcp"
        }],
        "essential": true
    }],
    "placementConstraints": [{
        "expression": "attribute:env-type == dev",
        "type": "memberOf"
    }]
}
